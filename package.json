{
  "name": "image-panda",
  "productName": "Image Panda",
  "version": "0.1.0",
  "private": true,
  "description": "Cross-platform image compression tool, provides offline & tinyPNG compression",
  "main": "src/main.js",
  "keywords": [
    "cross-platform",
    "windows",
    "mac",
    "compression",
    "tinify",
    "tinyPNG",
