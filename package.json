  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theahmadzai/image-panda.git"
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@sentry/electron": "^2.0.0",
    "async": "^3.2.0",
    "electron-log": "^4.2.4",
    "filesize": "^6.1.0",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-pngquant": "^9.0.1",
    "imagemin-svgo": "^8.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "tinify": "^1.6.0-beta.2"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "electron": "^9.3.1",
    "electron-builder": "^22.8.1",
    "electron-is-dev": "^1.2.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-standard": "^4.0.1",
    "stylelint": "^13.7.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "wait-on": "^5.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:styles": "stylelint src",
    "format:styles": "stylelint src --fix",
    "lint:scripts": "eslint src",
    "format:scripts": "eslint src --fix",
    "electron-start": "electron .",
    "electron-dev": "concurrently \"npm run start\" \"wait-on http://127.0.0.1:3000 && electron .\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
